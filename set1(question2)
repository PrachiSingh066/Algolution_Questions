#include <iostream>
using namespace std;
int main() {
    int num[5];
    cout << "Enter 5 integers: ";
    for (int i = 0; i < 5; i++) {
        cin >> num[i];
    }
    int max = num[0];
    for (int i = 1; i < 5; i++) {
        if (num[i] > max) {
            max = num[i];
        }
    }
    cout << "The maximum no. is: " << max << endl;
    return 0;
}
